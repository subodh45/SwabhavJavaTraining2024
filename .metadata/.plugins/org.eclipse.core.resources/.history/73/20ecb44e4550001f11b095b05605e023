package com.aurionpro.structural.proxy.model;

public class IEmployeeProxy implements IEmployee{

	EmployeeTask employee;
	
	
	public IEmployeeProxy(EmployeeTask employee) {
		super();
		this.employee = employee;
	}
	
	

	public EmployeeTask getEmployee() {
		return employee;
	}



	public void setEmployee(EmployeeTask employee) {
		this.employee = employee;
	}



	public void create(String role , Employee employee) {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void delete(String role , int empId) {
		// TODO Auto-generated method stub
		System.out.println("Remove employee of id "+ empId);
	}

	@Override
	public Employee get(String role , int empId) {
		System.out.println("fetching data ");
		return new Employee();
	}

}
