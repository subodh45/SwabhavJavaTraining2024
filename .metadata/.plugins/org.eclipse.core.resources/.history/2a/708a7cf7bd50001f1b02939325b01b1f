package com.aurionpro.behaviour.state.model;

public class Packet  {

	IPacketState state;

	public Packet(IPacketState state) {
		super();
		this.state = state;
	}
	
	public void gotoNextState()
	{
		state.next(this);
		this.state = (IPacketState) new Packet(new Dispatched());
	}
	
	public void gotoPreviousState()
	{
		state.previous(this);
	}
	
	public void currentState()
	{
		state.current(this);
	}
}
