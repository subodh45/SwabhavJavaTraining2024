public class HomeThreater implements IDVDPlayer {

	@Override
	public void DVDOn() {
		// TODO Auto-generated method stub
	   System.out.println("DVD on.");	
	}

	@Override
	public void DVDoff() {
		// TODO Auto-generated method stub
		System.out.println("DVD turn OFF");
	}

	@Override
	public void DVDstart() {
		// TODO Auto-generated method stub
		System.out.println("DVD START");
	}

	@Override
	public void DVDstop() {
		// TODO Auto-generated method stub
		System.out.println("dvd stop");
	}

	
}
