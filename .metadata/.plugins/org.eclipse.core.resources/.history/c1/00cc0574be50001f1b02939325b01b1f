package com.aurionpro.behaviour.state.model;

public class Packet  {

	IPacketState state;

	public Packet(IPacketState state) {
		super();
		this.state = state;
	}
	
	public void gotoNextState(Packet packet)
	{
		state.next(packet);
	}
	
	public void gotoPreviousState(Packet packet)
	{
		state.previous(packet);
	}
	
	public void currentState()
	{
		state.current(packet);
	}
}
