package com.aurionpro.game.model;

public class Board {

	Marker board[][];

	public Board(Marker[][] board) {
		super();
		this.board = board;
	}
	
	public void display()
	{
		for(int i=0;i<3;i++)
		{   
			for(int j=0;j<3;j++)
			{
			System.out.print(board[i][j]+"\t");
			}
			System.out.println();
		}
	}
}
