public class ICarTest {
public static void main(String[] args) {
	
	ICar car ;
	car = new Maruti();
	car.start();
	car.stop();
	
	car = new Tata();
	car.start();
	car.stop();
	
	car = new Mahindra();
	car.start();
	car.stop();
}
}
